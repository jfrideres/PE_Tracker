<!DOCTYPE html>
<html lang="en">
<head>
  <title>DB Info</title>
  <meta charset="UTF-8">
</head>
<body>
  <h1>Database Tables</h1>
  <ul>
    <% tables.forEach(function(r) { %>
       <li><%= r.table %>: <%= r.column %> (<%= r.type %>)</li>
    <% }); %>
  </ul>
  <h1>Observations</h1>
  <ul>
    <% obs.forEach(function(r) { %>
    <li><%= r.tasks_id %> for <%= r.duration.minutes || "00" %>:<%= r.duration.seconds || "00" %></li>
    <% }); %>
  </ul>
  
  <h1>Observation Information</h1>
  <ul>
    <% obsInfo.forEach(function(r) { %>
    <li>Observer's Name:<%= r.observerName %></li> 
    <li>Instructor's Name:<%= r.instructorName %></li>
    <li>School Name:<%= r.schoolName %></li>
    <li>Date:<%= r.observationDate %></li>
    <li>Grade:<%= r.grade %></li>
    <li>Topic:<%= r.topic %></li>
    <li>Student:<%= r.student %></li>
    <% }); %>
  </ul>
</body>
</html>